<h1>Movies</h1>
    <ul class="movies">
        {{#each moviesList}}
            <li class="movies-item">
                <h2>{{this.title}}</h2>
                <img src="{{this.image}}" alt="{{this.title}} Image">
                <a href="/movies/{{this._id}}">See more</a>
            </li>
        {{/each}}
    </ul>